{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "IMX Fee Remover",
    "version": "1.0",
    "description": "Remove Immutable X marketplace Maker & Taker fees.",
    "permissions": ["storage", "declarativeNetRequest"],
    "host_permissions": [
        "*://*.immutable.com/*"
    ],
    "icons": {
        "128": "icon128.png"
    },
    "background": {
        "service_worker": "script.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}